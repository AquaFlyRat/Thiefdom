            <string>/// clickable
var text = "start client";
var font = spr_font_b;
var scale = 1;

var vz = obj_view.view_zoom;
var mousex = (mouse_x-view_xview)*vz;
var mousey = (mouse_y-view_yview)*vz;
var vw = view_wview*obj_view.view_zoom;
var vh = view_hview*obj_view.view_zoom;

var textw = text_length(text, font, scale);
var texth = text_height(font, scale);

var textx = (vw / 2) - (textw / 2);
var texty = (vh *.75) - (texth / 2);
texty += text_height(font, scale) * 2.6;

if (abs(mousex-(textx+textw/2))&lt;textw/2 &amp;&amp; (mousey &gt; texty-texth / 2 &amp;&amp; mousey &lt; texty + texth / 2)) {   
    if(not self.started) {
        debug_message("Starting local client");
        var client_instance = instance_create(0,0,obj_local_client);
        client_instance.socket_id = create_client_socket();
        debug_message("Client Socket: " + string(client_instance.socket_id));
        
        client_start_requesting_connection(client_instance);
    }
    
    self.started = true;
}
</string>
