            <kind>1</kind>
            <string>///movement
var hs = (keyboard_check_direct(vk_right)-keyboard_check_direct(vk_left));
var jmp = (keyboard_check_pressed(vk_up));
var vs = (keyboard_check_direct(vk_down)-keyboard_check_direct(vk_up));
if hs !=0{hspd = lerp(hspd,hs*run_speed,.1)}else{hspd = lerp(hspd,0,.5)}
//grav, jump
if !climb &amp;&amp; !slide{
    vspd+=grav;
}else{
    if climb{
        if vs&lt;0{vspd = -climb_speed}
        if vs&gt;0{climb = 0; slide = 1;}
    }
    if slide{
        if vs&gt;0{vspd = slide_speed}
        if vs&lt;0{slide = 0; climb = 1;}
    }
    if vs ==0 &amp;&amp; (!slide) {vspd = 0}
}
if place_meeting(x,y+1,obj_solid) &amp;&amp; jmp{
    vspd=-jump_speed;
}
//colision x
if place_meeting(x+hspd,y,obj_solid){
    while !place_meeting(x+sign(hspd),y,obj_solid){
        x+=sign(hspd);
    }
    hspd = 0;
}
x+=hspd;
//colision y
if place_meeting(x,y+vspd,obj_solid){
    while !place_meeting(x,y+sign(vspd),obj_solid){
        y+=sign(vspd);
    }
    vspd = 0;
}
y+=vspd;
</string>
